
public class Box4 {

	double width;
	double height;
	double length;
	
	Box4(double w, double h,double l)
	{
		width= w;
		height=h;
		length=l;
		
	}
	
	double volume()
	{
		return width*height*length;
	}
	public static void main(String args[])
	{
		Box4 b1=new Box4(12.5,10,11);
		System.out.println(b1.volume());
	}
}
