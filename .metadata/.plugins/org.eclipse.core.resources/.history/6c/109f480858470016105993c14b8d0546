/* Class with Method with return type*/
public class Box1 {
	
	double width;
	double length;
	double height;
	
	double displayvolume()
	{
		return width*length*height;
		//System.out.println("The Volume of Box is "+(width*length*height));
		
	}
	
	double displayparameter()
	{
		return width+length+height;
		//System.out.println("The parameter of the Box is " +(width+length+height));
	}
	
	public static void main(String args[])
	{
		Box1 b1=new Box1();
		Box1 b2=new Box1();
		b1.width=10;
		b1.length=12;
		b1.height=5.32;
		
		b2.width=11;
		b2.length=12;
		b2.height=13;
		
		b1.displayvolume();
		b1.displayparameter();
		b2.displayvolume();
		b2.displayparameter();
		
	}
}